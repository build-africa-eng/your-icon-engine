{
  "name": "@nyigoro/icon-engine",
  "version": "2.0.0",
  "description": "Utility Icon Engine for matching utility-like strings (e.g. Tailwind, Docker, etc.) to icons from multiple libraries.",
  "type": "module",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:lib": "tsc -p tsconfig.build.json"
  },
  "keywords": [
    "icons",
    "react",
    "vite",
    "utility-icons",
    "icon-engine",
    "lucide",
    "heroicons",
    "tabler",
    "phosphor",
    "iconoir",
    "tailwind",
    "docker"
  ],
  "author": "African Future (Nyigoro)",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@phosphor-icons/react": "^2.1.10",
    "@tabler/icons-react": "^3.34.0",
    "iconoir-react": "^7.11.0",
    "lucide-react": "^0.514.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^6.3.5"
  }
}